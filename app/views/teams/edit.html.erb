<h1>Editing team</h1>

<%= form_with model: @team do |frm| %>
  
  <div class="field"> 
    <%= frm.label :name %>
    <%= frm.text_field :name %>
  </div>
  <br>

  <%= frm.submit %>
  <br>

<% end %>
<br>

<div>
  <%= link_to "Show this team", @team %> |
  <%= link_to "Back to teams", teams_path %>
</div>
